      <!--<map:match pattern="**/*-snapshot.png">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml" label="xml"/>
        <map:transform type="xslt2" src="processing/steps/sonnet-snapshot.xsl"/>
        <map:serialize type="svg2png"/>
      </map:match>-->
      <!--<map:match pattern="**/*-sonnet-arcs.svg">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml" label="xml"/>
        <map:transform type="xslt2" src="processing/steps/sonnet-arcs-svg.xsl"/>
