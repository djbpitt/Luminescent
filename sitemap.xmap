<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">
  <map:components>
    <map:generators default="file"/>
    <map:transformers default="xslt2"/>
    <map:serializers default="xml">
      <map:serializer name="text"
        src="org.apache.cocoon.serialization.TextSerializer"
        mime-type="text/plain" logger="sitemap.serializer.text">
        <encoding>UTF-8</encoding>
      </map:serializer>
      <map:serializer name="xhtml"
        src="org.apache.cocoon.serialization.XMLSerializer"
        mime-type="text/html" logger="sitemap.serializer.xhtml" pool-max="64">
        <doctype-public>-//W3C//DTD XHTML 1.0 Strict//EN</doctype-public>
        <doctype-system>http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd</doctype-system>
        <omit-xml-declaration>true</omit-xml-declaration>
        <encoding>UTF-8</encoding>
      </map:serializer>
      <map:serializer name="xml" mime-type="text/xml"
        src="org.apache.cocoon.serialization.XMLSerializer">
        <encoding>UTF-8</encoding>
      </map:serializer>
      <map:serializer name="html" mime-type="text/html; charset=UTF-8"
        src="org.apache.cocoon.serialization.HTMLSerializer">
        <encoding>UTF-8</encoding>
        <!-- the following common doctype is only included for completeness;
             it has no impact on encoding -->
        <doctype-public>-//W3C//DTD HTML 4.01 Transitional//EN</doctype-public>
        <doctype-system>http://www.w3.org/TR/html4/loose.dtd</doctype-system>
      </map:serializer>
      <map:serializer name="svgxml" mime-type="image/svg+xml"
        logger="sitemap.serializer.svgxml"
        src="org.apache.cocoon.serialization.XMLSerializer">
        <encoding>UTF-8</encoding>
        <!--<doctype-public>-//W3C//DTD SVG 1.0//EN</doctype-public>
          <doctype-system>http://www.w3.org/TR/2001/REC-SVG-20010904/DTD/svg10.dtd</doctype-system>-->
      </map:serializer>
    </map:serializers>
    <map:readers default="resource"/>
    <map:matchers default="wildcard"/>
    <map:selectors default="browser"/>
  </map:components>
  <map:pipelines>
    <map:pipeline type="noncaching">
      <!-- SWITCH TO CACHING WHEN EVERYTHING IS WORKING -->
      <map:match pattern="**/*.js">
        <map:read mime-type="text/ecmascript" src="bin/support/{2}.js"/>
      </map:match>
      <map:match pattern="processor">
        <map:generate src="bin/support/l.xml"/>
        <map:transform type="xslt2" src="bin/support/processor-version.xsl"/>
        <map:serialize type="html"/>
      </map:match>
      <map:match pattern="sitemap.html">
        <map:generate src="sitemap.xmap"/>
        <map:transform type="xslt2"
          src="bin/support/infrastructure-display-html.xsl">
          <map:parameter name="level" value="."/>
        </map:transform>
        <map:serialize type="html"/>
      </map:match>
      <map:match pattern="sitemap.xmap">
        <map:generate src="sitemap.xmap"/>
        <map:serialize type="xml"/>
      </map:match>
      <map:match pattern="**.xsl">
        <map:generate src="{1}.xsl"/>
        <map:transform type="xslt2"
          src="bin/support/infrastructure-display-html.xsl">
          <map:parameter name="level" value="."/>
        </map:transform>
        <map:serialize type="html"/>
      </map:match>

      <map:match pattern="**.lmnl">
        <!--<map:read mime-type="text/plain" src="{1}.lmnl"/>-->
        <map:generate src="bin/support/l.xml"/>
        <map:transform type="xslt2" src="bin/support/plaintext.xsl">
          <map:parameter name="plaintext-file" value="../../{1}.lmnl"/>
        </map:transform>
        <map:serialize type="text"/>
      </map:match>

      <!--<map:match pattern="**.wlmnl">
        <map:aggregate element="lmnl">
          <map:part src="cocoon:/{1}.lmnl"/>
        </map:aggregate>
        <map:serialize type="xml"/>
      </map:match>-->

      <map:match pattern="**/xLMNL/*.xml">
        <map:generate src="bin/support/l.xml" label="wrapped"/>
        <map:transform type="xslt2" label="hide-comments"
          src="bin/up/hide-comments.xsl">
          <map:parameter name="lmnl-file" value="../../{1}/{2}.lmnl"/>
        </map:transform>
        <map:transform type="xslt2" label="tokenize"
          src="bin/up/tokenize.xsl"/>
        <map:transform type="xslt2" label="locate-tokens"
          src="bin/up/locate-tokens.xsl"/>
        <map:transform type="xslt2" label="tag-sawteeth"
          src="bin/up/tag-teeth.xsl"/>
        <map:transform type="xslt2" label="assign-types"
          src="bin/up/assign-types.xsl"/>
        <map:transform type="xslt2" label="label-starts"
          src="bin/up/label-starts.xsl"/>
        <map:transform type="xslt2" label="label-ends"
          src="bin/up/label-ends.xsl"/>
        <map:transform type="xslt2" label="infer-annotations"
          src="bin/up/infer-annotations.xsl"/>
        <map:transform type="xslt2" label="mark-offsets"
          src="bin/up/mark-offsets.xsl"/>
        <map:transform type="xslt2" label="extract-labels"
          src="bin/up/extract-labels.xsl"/>
        <map:transform type="xslt2" label="assign-ids"
          src="bin/up/assign-ids.xsl"/>
        <map:transform type="xslt2" label="mark-spans"
          src="bin/up/mark-spans.xsl"/>

        <map:transform type="xslt2" label="tags-to-xlmnl"
          src="bin/up/tags-to-xlmnl.xsl"/>

        <map:serialize type="xml"/>
      </map:match>
      <map:match pattern="**/*-analysis.html">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml"/>
        <map:transform type="xslt2" src="bin/down/xLMNL-analyze.xsl"/>
        <map:serialize type="html"/>
        <!--<map:serialize type="xml"/>-->
      </map:match>
      <map:match pattern="**/*-sonneteer.svg">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml"/>
        <map:transform type="xslt2" src="bin/down/sonneteer-svg.xsl"/>
        <map:serialize type="svgxml"/>
        <!--<map:serialize type="xml"/>-->
      </map:match>
      <map:match pattern="**/*-sonneteer.html">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml"/>
        <map:transform type="xslt2" src="bin/down/sonneteer-graph.xsl"/>
        <map:serialize type="xhtml"/>
      </map:match>
      <map:match pattern="**/*-graph.svg">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml"/>
        <map:transform type="xslt2" src="bin/down/{2}-graph-svg.xsl"/>
        <map:serialize type="svgxml"/>
        <!--<map:serialize type="xml"/>-->
      </map:match>
      <map:match pattern="**/*.xml">
        <map:generate src="cocoon:/{1}/xLMNL/{2}.xml"/>
        <map:transform type="xslt2" src="bin/down/xlmnl-xml.xsl">
          <map:parameter name="use-request-parameters" value="true"/>
        </map:transform>
        <map:serialize type="xml"/>
      </map:match>
      <map:match pattern="**/demo.html">
        <map:generate src="cocoon:/{1}/xLMNL/demo.xml"/>
        <map:transform type="xslt2" src="bin/down/xlmnl-xml.xsl">
          <map:parameter name="element-list" value="demo+div+head+p+list+item+emph+term+q+link"/>
        </map:transform>
        <map:transform type="xslt2" src="bin/down/demo-html.xsl"/>
        <map:serialize type="html"/>
      </map:match>
      <!-- Following are leftover scraps ... -->


