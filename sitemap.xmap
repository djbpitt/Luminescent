    </map:view>-->

    <!-- these take SVG input -->
    <map:view name="jpeg" from-position="last">
      <map:serialize type="svg2jpeg"/>
    </map:view>
    <map:view name="png" from-position="last">
      <map:serialize type="svg2png"/>
    </map:view>
  </map:views>
</map:sitemap>
