      <map:match pattern="*/directory">
        <map:generate src="{1}" type="xpathdirectory">
          <map:parameter name="xpath" value="/*/meta"/>
          <map:parameter name="xmlFiles" value="\.xml$"/>
        </map:generate>
        <map:serialize type="xml"/>
      </map:match>
      <!-- <map:match pattern="**/*">
        <map:generate src="examples/{1}/{2}" type="directory"/>
        <map:transform type="xslt2" src="processing/support/directory-links.xsl">
          <map:parameter name="dir" value="{2}"/>
          <map:parameter name="use-request-parameters" value="true"/>
        </map:transform>
        <map:serialize type="html"/>
        </map:match>-->
      <map:match pattern="*/index.html">
        <map:redirect-to uri="cocoon:/{1}"/>
      </map:match>
      <map:match pattern="*">
        <map:generate src="cocoon:/{1}/directory"/>
        <map:transform type="xslt2" src="bin/support/directory-links.xsl">
          <map:parameter name="dir" value="{1}"/>
          <map:parameter name="use-request-parameters" value="true"/>
        </map:transform>
        <map:serialize type="html"/>
      </map:match>
      <map:handle-errors>
        <map:generate type="exception"/>
        <map:serialize type="xml"/>
      </map:handle-errors>
    </map:pipeline>
  </map:pipelines>
  <map:views>
    <map:view name="step0" from-label="hide-comments">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step1" from-label="tokenize">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step2" from-label="locate-tokens">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step3" from-label="tag-sawteeth">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step4" from-label="assign-types">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step5" from-label="label-starts">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step6" from-label="label-ends">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step7" from-label="infer-annotations">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step8" from-label="mark-offsets">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step9" from-label="extract-labels">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step10" from-label="assign-ids">
      <map:serialize type="xml"/>
    </map:view>
    <map:view name="step11" from-label="mark-spans">
      <map:serialize type="xml"/>
    </map:view>

